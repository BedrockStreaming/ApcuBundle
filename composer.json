{
    "name": "m6web/apcu-bundle",
    "version": "dev-0.1",
    "type": "symfony-bundle",
    "description" : "Provide APCu support",
    "require": {
        "php"                            : ">=7.4",
        "ext-apcu"                       : ">=4.0.7"
    },
    "require-dev": {
        "phpunit/phpunit"                : "^9.5",
        "m6web/php-cs-fixer-config"      : "^2.0",
        "m6web/guzzle-http-bundle"       : "^4.0 || ^5.0",
        "guzzle/cache"                   : "^3.9.2"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "M6Web\\ApcuBundle\\Tests\\":  "tests" }
    },
    "minimum-stability" : "dev"
}
